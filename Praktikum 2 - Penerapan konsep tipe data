        "#tipe data Integer\n",
        "print(20)\n",
        "\n",
        "#tipe data Float\n",
        "print(3.14)\n",
        "print(.2)\n",
        "print(4.2e-3)\n",
        "\n",
        "#tipe data Binary\n",
        "print(0b10)\n",
        "\n",
        "#tipe data octal\n",
        "print(0o10)\n",
        "\n",
        "#tipe data Hexadecimal\n",
        "print('tipe data heksa desimal',0x10)\n",
        "\n",
        "#tipe data Complex\n",
        "print(5j)\n",
        "\n",
        "#tipe data List\n",
        "print([1,2,3,4,5])\n",
        "print([\"satu\", \"dua\", \"tiga\"])\n",
        "\n",
        "#tipe data Tuple\n",
        "print((1,2,3,4,5))\n",
        "print((\"satu\", \"dua\", \"tiga\"))"
